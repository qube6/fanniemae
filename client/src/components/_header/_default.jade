          li
            a(href='http://homepath.com', target='_blank')
              h2 HomePath
              .subhead Find & Buy Homes
    form.header-search-form.desktop-only(action="/" method="post" accept-charset="UTF-8" ng-class="{ 'header-search-open' : isActive('header-search') }")
      input.header-search-input(placeholder="Search" type="search" ng-model="header.search" ng-change="setHeaderSubmit('header-submit')")
      input.header-search-submit(type="submit" value="" ng-class="{ 'header-search-active' : isActive('header-submit') }")
      a.header-search-btn(ng-click="toggleActive('header-search')")
