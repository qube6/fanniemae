header#navbar.navbar.navbar-default(role="banner") 
  .navbar-header
    button.navbar-toggle.pull-right(type='button' ng-click="toggleActive('header-mobile-menu')")
      span.sr-only Toggle navigation
      span.icon-bar.one
      span.icon-bar.two
      span.icon-bar.three
      span.hamburger-text MENU
    a.site-logo.navbar-btn.pull-left(href="/" title="home")
      svg.logo(role="img" title="Fannie ae" viewBox="0 0 375.4 74.5" preserveAspectRatio="none")
        use(xlink:href="/media/FannieMae-Logo.svg#Logo")
  nav.navbar-offcanvas.pull-right
    .menu-sites(ng-class="{ 'open' : isActive('header-side-menu') }" fm-close-on-click-away='header-side-menu')
      button.toggle-sites.desktop-only(ng-click="toggleActive('header-side-menu')")
        | All Fannie Mae Sites
        i
      .sites-list
        h1 Doing Business with Fannie Mae
        ul.menu.nav
          li
            h2 Lenders
            button.dropdown-toggle(fm-toggle-parent-on-click)
            ul.submenu
              li
                a(href='https://www.fanniemae.com/singlefamily/index', target='_blank') Single Family
              li
                a(href='https://www.fanniemae.com/multifamily/index', target='_blank') Multi Family
          li
            h2 Servicers
            button.dropdown-toggle(fm-toggle-parent-on-click)
            ul.submenu
              li
                a(href='https://www.fanniemae.com/singlefamily/servicing', target='_blank') Single Family
              li
