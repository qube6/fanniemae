fm-accordion.fm-accordion
  h1.fm-subtitle Board Committees
    button.fm-accordion-toggle-all(type="button" ng-click="setAll()")
      div(ng-show="allOpen") Close All
      div(ng-show="!allOpen") Expand All
      span.icon.list-icon(ng-class=" allOpen ? 'fm-minus-circle' : 'fm-plus-circle' " aria-hidden="true")
  ul.link-listing.custom-icons.bottom-border
    //- li(fm-accordion-item ng-class="{ 'open' : isOpen() }" open)
